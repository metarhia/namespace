{
  "name": "namespace",
  "version": "0.0.1",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "High stable namespace registry",
  "license": "MIT",
  "keywords": [
    "namespace",
    "metarhia",
    "npm",
    "registry",
    "repository",
    "algorithms",
    "modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/namespace"
  },
  "main": "./namespace.js",
  "readmeFilename": "README.md",
